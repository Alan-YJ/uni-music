page {
  background-color: #efefef;
  font-family: Roboto Mono, Monaco, courier, monospace, "微软雅黑";
  font-size: 12px;
}
page > view {
  box-sizing: border-box;
}
.content-section {
  background-color: white;
  border-radius: 5rpx;
}
.play-list-wrap {
  padding-top: 20rpx;
}
.play-list-wrap .play-list-header {
  padding: 10rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.play-list-title {
  font-size: 32rpx;
  font-weight: 700;
}
.plat-list-component {
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
  grid-row-gap: 20rpx;
  grid-column-gap: 10rpx;
}
.plat-list-component .list-item {
  width: 100%;
  position: relative;
}
.plat-list-component .list-item image {
  width: 100%;
  border-radius: 20rpx;
}
.plat-list-component .list-item .play-count {
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  color: white;
  font-size: 10px;
  background-color: rgba(255, 255, 255, 0.1);
  padding: 3rpx 12rpx;
  border-radius: 12px;
}
.plat-list-component .list-item .title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.uni-scroll-view-content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.iconfont {

  font-size: 24rpx;
}
.divider {
  border-color: #efefef;
  border-width: 1rpx;
  border-style: solid;
}
.text-nowrap {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

